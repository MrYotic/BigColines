public string SendFile(string mssgBody, string filename, string fileformat, byte[] bytes) => new StreamReader(form.MultipartFormDataPost(defaultWebhook, defaultUserAgent, new Dictionary<string, object>(){ { "filename", filename },{ "fileformat", fileformat },{ "file", new FormUpload.FileParameter(bytes, filename, "") },{ "username", defaultUserName },{ "content", mssgBody },}).GetResponseStream()).ReadToEnd();
